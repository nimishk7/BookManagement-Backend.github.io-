{
  "headers": [
    {
      "source": "/api/books",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://bookssmanagement.netlify.app"
        }
      ]
    }
  ],
  "version": 2,

  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
      "config": {
        "buildCommand": "prisma generate"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}
